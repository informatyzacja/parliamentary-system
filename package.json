{
  "name": "parliamentary-system",
  "private": true,
  "version": "1.4.41",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:db": "docker compose -f docker-compose.dev.yml up --force-recreate -V",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "lint": "turbo run lint",
    "postinstall": "husky install",
    "preinstall": "npx npm-force-resolutions",
    "semantic-release": "semantic-release"
  },
  "packageManager": "npm@9.6.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/informatyzacja/parliamentary-system.git"
  },
  "author": {
    "name": "Michał Korczak",
    "url": "https://omikor.in"
  },
  "maintainers": [
    {
      "name": "Michał Korczak",
      "url": "https://omikor.in"
    },
    {
      "name": "Dawid Stachowiak"
    },
    {
      "name": "Bartosz Gotowski"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/informatyzacja/parliamentary-system/issues"
  },
  "homepage": "https://github.com/informatyzacja/parliamentary-system#readme",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "semantic-release": "^21.0.7",
    "turbo": "^1.10.9",
    "wait-on": "^7.0.1"
  },
  "resolutions": {
    "graphql": "16.6.0",
    "**/graphql": "16.6.0"
  },
  "overrides": {
    "graphql": {
      ".": "16.6.0"
    }
  },
  "engines": {
    "node": "^18.17.0"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.8.1"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx,graphql,gql}": "eslint --ext .js,.jsx,.ts,.tsx,.graphql,.gql "
  }
}
