{
  "kind": "collectionType",
  "collectionName": "meetings",
  "info": {
    "singularName": "meeting",
    "pluralName": "meetings",
    "displayName": "Meeting",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
