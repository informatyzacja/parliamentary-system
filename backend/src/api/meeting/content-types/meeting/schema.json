      "target": "api::term-of-office.term-of-office",
      "inversedBy": "meetings"
    },
    "agenda": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "protocol": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "reports": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "resolutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resolution.resolution",
      "mappedBy": "meeting"
